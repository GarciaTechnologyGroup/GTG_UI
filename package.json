{
  "name": "garcia-technology-group",
  "version": "1.0.0",
  "description": "Garcia Technology Group LLC",
  "main": "index.js",
  "repository": "git@github.com:richardjboothe/garcia_tech.git",
  "scripts": {
    "build": "npm run clean-dist && babel-node main/tools/build.js",
    "clean-dist": "npm run remove-dist && mkdir -p dist/",
    "lint": "esw webpack.config.* main main/tools --color --fix --quiet",
    "lint-staged": "lint-staged",
    "open-dist": "babel-node main/tools/distServer.js",
    "open-src": "babel-node main/tools/srcServer.js",
    "precommit": "npm run lint-staged -s",
    "prepush": "npm run test",
    "remove-dist": "rimraf ./dist",
    "start": "npm-run-all --parallel open-src",
    "start-message": "babel-node main/tools/startMessage.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.0",
    "babel-plugin-import": "^1.2.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.18.12",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "lint-staged": "^3.6.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.5.10",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-preset-stage-1": "^6.1.18",
    "classnames": "^2.2.5",
    "event-stream": "^3.3.4",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "lodash.throttle": "^4.1.1",
    "nodemon": "^1.11.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  }
}
